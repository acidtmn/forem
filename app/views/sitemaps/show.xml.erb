<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <%= @articles.each do |article_obj| %>
    <url>
        <loc><%= app_protocol_and_domain %><%= article_obj[0] %></loc>
        <lastmod><%= article_obj[1] %></lastmod>
    </url>
  <% end %>
</urlset>
