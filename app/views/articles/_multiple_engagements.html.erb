<div class="multiple_reactions_engagement">
  <% ReactionCategory.for_view.each do |reaction_type| %>
    <span class="reaction_engagement_<%= reaction_type.slug %>">
      <%= crayons_icon_tag(reaction_type.icon, native: true, aria_hidden: true) %>
      <span id="reaction_engagement_<%= reaction_type.slug %>_count">&nbsp;</span>
    </span>
  <% end %>
</div>
