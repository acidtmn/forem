<div
  role="group"
  aria-label="Pagination group of buttons"
  class="crayons-btn-group hidden"
  id="btn-pagination-container">
  <button
    id="previous-page"
    type="button"
    class="c-btn c-btn--icon-alone crayons-tooltip__activator"
    aria-label="Previous Page">
    <svg
      class="crayons-icon"
      tabindex="0"
      width="24"
      height="24"
      viewBox="0 0 24 24"
      fill="none"
      xmlns="http://www.w3.org/2000/svg">
      <path d="m10.828 12 4.95 4.95-1.414 1.414L8 12l6.364-6.364 1.414 1.414-4.95 4.95Z" />
    </svg>
    <span data-testid="tooltip" class="crayons-tooltip__content">
      Previous Page
    </span>
  </button>
  <div class="page-btn-container-js">
  </div>
  <button
    id="next-page"
    type="button"
    class="c-btn c-btn--icon-alone crayons-tooltip__activator"
    aria-label="Next Page">
    <svg
      class="crayons-icon"
      tabindex="0"
      width="24"
      height="24"
      viewBox="0 0 24 24"
      xmlns="http://www.w3.org/2000/svg">
      <path d="m13.172 12-4.95-4.95 1.414-1.414L16 12l-6.364 6.364-1.414-1.414 4.95-4.95Z" />
    </svg>
    <span data-testid="tooltip" class="crayons-tooltip__content">
      Next Page
    </span>
  </button>
</div>
