<%- headers = ["Name", "Username", "Email address", "Status", "Joining date", "Last activity", "Organizations"] -%>
<%= CSV.generate_line headers %>
<%- @users.each do |user| -%>
  <%- row = [user.name, user.username, user.email, "status", user.registered_at.strftime("%d %b, %Y"), user.last_activity, user.organizations.pluck(:name)] -%>
  <%= CSV.generate_line(row).html_safe %>
<%- end -%>